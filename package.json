    "gridfs-stream": "^1.1.1",
    "jwt-simple": "^0.5.1",
    "md5": "^2.2.1",
    "mime": "^1.3.6",
    "mkdirp": "^0.5.1",
    "mongoose": "^4.9.8",
    "morgan": "^1.8.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.0.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "nodemon": "^1.11.0",
